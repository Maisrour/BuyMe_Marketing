{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "4dc29119-0fec-40ab-a96b-0c94f047c5a8"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "BuyMe": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/BuyMe",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/eShop/js/jquery.min.js",
              "src/assets/eShop/js/jquery-migrate-3.0.0.js",
              "src/assets/eShop/js/jquery-ui.min.js",
              "src/assets/eShop/js/popper.min.js",
              "src/assets/eShop/js/bootstrap.min.js",
              "src/assets/eShop/js/slicknav.min.js",
              "src/assets/eShop/js/owl-carousel.js",
              "src/assets/eShop/js/magnific-popup.js",
              "src/assets/eShop/js/waypoints.min.js",
              "src/assets/eShop/js/finalcountdown.min.js",
              "src/assets/eShop/js/nicesellect.js",
              "src/assets/eShop/js/flex-slider.js",
              "src/assets/eShop/js/scrollup.js",
              "src/assets/eShop/js/onepage-nav.min.js",
              "src/assets/eShop/js/easing.js",
              "src/assets/eShop/js/active.js"

            ],
            "serviceWorker": true,
            "ngswConfigPath": "ngsw-config.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "BuyMe:build:production"
            },
            "development": {
              "browserTarget": "BuyMe:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "BuyMe:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "BuyMe"
}
